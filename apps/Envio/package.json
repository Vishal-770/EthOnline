{
  "name": "envio",
  "version": "1.0.0",
  "description": "Envio Backend API for Token Analytics",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc && node -e \"console.log('Build completed successfully!')\"",
    "build:clean": "rimraf dist && npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist",
    "prod": "npm run build && npm start"
  },
  "keywords": [
    "envio",
    "blockchain",
    "token-analytics"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@envio-dev/hypersync-client": "^0.6.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/redis": "^4.0.10",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "redis": "^5.9.0",
    "viem": "^2.38.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
