    "test": "jest",
    "health-check": "node test-system-health.js",
    "memesentinel": "tsx src/orchestrator.ts",
    "memesentinel:build": "tsc && node dist/orchestrator.js",
    "scout": "tsx src/agents/scout.ts",
    "yield": "tsx src/agents/yield.ts",
    "risk": "tsx src/agents/risk.ts",
    "alert": "tsx src/agents/alert.ts",
    "settlement": "tsx src/agents/settlement.ts",
    "assistant": "tsx src/agents/assistant.ts"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.4",
    "@hashgraph/sdk": "^2.68.0",
    "@langchain/core": "^0.3.66",
    "@langchain/groq": "0.2.3",
    "@langchain/openai": "^0.3.12",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "express": "^4.21.1",
    "langchain": "^0.3.7",
    "node-cron": "^3.0.3",
    "uuid": "^11.1.0",
    "ws": "^8.18.0"
